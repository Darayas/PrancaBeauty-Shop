@page "/{culture}/Search/Compo/PropertisSearch"
@model PrancaBeauty.WebApp.Pages.Home.Search.Components.Compo_Propertis.Compo_PropertisSearchModel

@foreach (var item in Model.Data)
{
    <div class="widget_list widget_color">
        <h3>@item.Title</h3>
        <ul>
            @foreach (var itemValue in item.ValueItems)
            {
                <li>
                    <input type="checkbox" value="@itemValue.Title" /> <span>@itemValue.Title</span>
                </li>
            }
        </ul>
    </div>
}